{
	"name": "@eslint-sets/monorepo",
	"description": "eslint-sets",
	"version": "4.0.0-beta.2",
	"packageManager": "pnpm@7.18.2",
	"scripts": {
		"eslint": "eslint --fix --ext .ts,.tsx,.js,.jsx ./",
		"prettier": "prettier --write ./",
		"reinstall": "tscjs scripts/reinstall",
		"lib": "sh scripts/lib.sh",
		"docs": "sh scripts/docs.sh --docs",
		"push": "tscjs scripts/push",
		"build": "tscjs scripts/build",
		"build:rollup": "cross-env NODE_OPTIONS=\"--max-old-space-size=6144\" rollup -c",
		"pub": "tscjs scripts/publish",
		"unpub": "tscjs scripts/unpublish",
		"sync": "tscjs scripts/sync",
		"viewInfo": "tscjs scripts/viewInfo",
		"update": "tscjs scripts/update",
		"dist": "pnpm run update && pnpm build",
		"clean": "rimraf dist types typings packages/*/dist"
	},
	"main": "index.js",
	"devDependencies": {
		"@babel/eslint-parser": "^7.19.1",
		"@rollup/plugin-commonjs": "^23.0.0",
		"@rollup/plugin-json": "^4.1.0",
		"@rollup/plugin-node-resolve": "^15.0.1",
		"@types/jest": "^29.0.0",
		"@types/node": "^18.7.16",
		"@typescript-eslint/eslint-plugin": "^5.36.2",
		"@typescript-eslint/parser": "^5.36.2",
		"chalk": "^5.0.1",
		"consola": "^2.15.3",
		"cross-env": "^7.0.3",
		"esbuild": "^0.16.10",
		"esbuild-register": "^3.3.3",
		"eslint": "^7.32.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-config-sets": "~1.8.3",
		"eslint-plugin-eslint-comments": "^3.2.0",
		"eslint-plugin-import": "^2.26.0",
		"eslint-plugin-jsonc": "^2.4.0",
		"eslint-plugin-markdown": "^3.0.0",
		"eslint-plugin-n": "^15.2.5",
		"eslint-plugin-prettier": "^4.2.1",
		"eslint-plugin-promise": "^5.2.0",
		"eslint-plugin-tsdoc": "^0.2.16",
		"eslint-plugin-unicorn": "^45.0.2",
		"eslint-plugin-yml": "^1.3.0",
		"fast-glob": "^3.2.11",
		"jest": "^29.0.2",
		"js-cool": "^2.8.0",
		"jsonc-eslint-parser": "^2.1.0",
		"npm-run-all": "^4.1.5",
		"prettier": "^2.7.1",
		"prettier-config-common": "^1.3.0",
		"reinstaller": "^1.1.0",
		"rimraf": "^3.0.2",
		"rollup": "^2.79.0",
		"rollup-plugin-add-banner": "^1.1.0",
		"rollup-plugin-dts": "^4.2.3",
		"rollup-plugin-esbuild": "^4.10.1",
		"rollup-plugin-filesize": "^9.1.2",
		"rollup-plugin-replace-shebang": "^1.2.0",
		"rollup-plugin-terser": "^7.0.2",
		"rollup-plugin-visualizer": "^5.8.1",
		"tsnd": "^1.1.0",
		"typedoc": "^0.23.14",
		"typedoc-plugin-markdown": "^3.13.6",
		"typescript": "~4.8.4",
		"vue-eslint-parser": "^9.1.0",
		"yaml-eslint-parser": "^1.1.0",
		"zx": "^7.0.8"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/saqqdy/eslint-sets.git"
	},
	"pnpm": {
		"peerDependencyRules": {
			"ignoreMissing": [
				"@babel/core",
				"webpack"
			],
			"allowedVersions": {
				"eslint": ">=7.0.0"
			}
		}
	},
	"publishConfig": {
		"registry": "https://registry.npmjs.org",
		"access": "public"
	},
	"keywords": [
		"eslint-sets",
		"vue",
		"vue3.0"
	],
	"author": "saqqdy.com",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/saqqdy/eslint-sets/issues"
	},
	"homepage": "https://github.com/saqqdy/eslint-sets#readme"
}
