{
    "name": "@eslint-sets/eslint-config-egg",
    "description": "Eslint config sets for eggjs",
    "version": "2.1.5",
    "main": "./index.js",
    "scripts": {},
    "directories": {
        "es": "es",
        "lib": "lib",
        "src": "src",
        "typings": "typings"
    },
    "dependencies": {
        "@eslint-sets/eslint-config-basic": "workspace:*",
        "eslint-config-egg": "^12.0.0",
        "eslint-config-prettier": "^8.5.0",
        "eslint-plugin-prettier": "^4.2.1"
    },
    "devDependencies": {
        "typescript": "^4.7.4"
    },
    "peerDependencies": {
        "eslint": ">=8.0.0",
        "prettier": ">=2.4.0"
    },
    "sideEffects": false,
    "repository": {
        "type": "git",
        "url": "git+https://github.com/saqqdy/eslint-sets.git",
        "directory": "packages/egg"
    },
    "pnpm": {
        "peerDependencyRules": {
            "ignoreMissing": [
                "@babel/core",
                "webpack"
            ],
            "allowedVersions": {
                "eslint": "^8.10.0"
            }
        }
    },
    "publishConfig": {
        "registry": "https://registry.npmjs.org",
        "access": "public"
    },
    "keywords": [
        "eslint-sets",
        "git",
        "tool"
    ],
    "author": "saqqdy <https://github.com/saqqdy>",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/saqqdy/eslint-sets/issues"
    },
    "homepage": "https://github.com/saqqdy/eslint-sets/tree/master/packages/egg#readme"
