{
	"name": "@eslint-sets/eslint-config-nuxt",
	"description": "Eslint config sets for nuxt",
	"version": "4.1.0-beta.5",
	"main": "./index.js",
	"scripts": {},
	"files": [
		"index.js",
		"typings",
		"*.js",
		"*.d.ts"
	],
	"directories": {
		"es": "es",
		"lib": "lib",
		"src": "src",
		"typings": "typings"
	},
	"dependencies": {
		"@eslint-sets/eslint-config-basic": "workspace:*",
		"@nuxtjs/eslint-config": "^8.0.0",
		"@nuxtjs/eslint-module": "^3.1.0",
		"eslint-config-prettier": "^8.6.0",
		"eslint-plugin-jsdoc": "^39.6.4",
		"eslint-plugin-nuxt": "^4.0.0",
		"eslint-plugin-prettier": "^4.2.1",
		"eslint-plugin-promise": "^6.1.1",
		"vue-eslint-parser": "^9.1.0"
	},
	"devDependencies": {
		"eslint": "^8.31.0",
		"prettier": "^2.8.3",
		"typescript": "~4.9.4"
	},
	"peerDependencies": {
		"eslint": ">=7.4.0",
		"prettier": ">=2.0.0",
		"typescript": ">=3.9.0"
	},
	"keywords": [
		"eslint-sets",
		"git",
		"tool"
	],
	"license": "MIT",
	"author": "saqqdy <https://github.com/saqqdy>",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/saqqdy/eslint-sets.git",
		"directory": "packages/nuxt"
	},
	"pnpm": {
		"peerDependencyRules": {
			"ignoreMissing": [
				"@babel/core",
				"webpack"
			],
			"allowedVersions": {
				"eslint": ">=7.4.0"
			}
		}
	},
	"publishConfig": {
		"registry": "https://registry.npmjs.org",
		"access": "public"
	},
	"bugs": {
		"url": "https://github.com/saqqdy/eslint-sets/issues"
	},
	"homepage": "https://github.com/saqqdy/eslint-sets/tree/master/packages/nuxt#readme"
}
