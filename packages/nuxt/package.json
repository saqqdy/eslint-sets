{
	"name": "@eslint-sets/eslint-config-nuxt",
	"description": "Eslint config sets for nuxt",
	"version": "4.0.0-beta.10",
	"main": "./index.js",
	"scripts": {},
	"directories": {
		"es": "es",
		"lib": "lib",
		"src": "src",
		"typings": "typings"
	},
	"dependencies": {
		"@eslint-sets/eslint-config-basic": "workspace:*",
		"@nuxtjs/eslint-config": "^8.0.0",
		"@nuxtjs/eslint-module": "^3.1.0",
		"eslint-config-prettier": "^8.6.0",
		"eslint-plugin-jsdoc": "^39.6.4",
		"eslint-plugin-nuxt": "^4.0.0",
		"eslint-plugin-prettier": "^4.2.1",
		"eslint-plugin-promise": "^5.2.0",
		"vue-eslint-parser": "^9.1.0"
	},
	"devDependencies": {
		"eslint": "^7.32.0",
		"prettier": "^2.8.2",
		"typescript": "~4.8.4"
	},
	"peerDependencies": {
		"eslint": ">=7.4.0",
		"prettier": ">=2.0.0",
		"typescript": ">=3.5.0"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/saqqdy/eslint-sets.git",
		"directory": "packages/nuxt"
	},
	"pnpm": {
		"peerDependencyRules": {
			"ignoreMissing": [
				"@babel/core",
				"webpack"
			],
			"allowedVersions": {
				"eslint": ">=7.4.0"
			}
		}
	},
	"publishConfig": {
		"registry": "https://registry.npmjs.org",
		"access": "public"
	},
	"keywords": [
		"eslint-sets",
		"git",
		"tool"
	],
	"author": "saqqdy <https://github.com/saqqdy>",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/saqqdy/eslint-sets/issues"
	},
	"homepage": "https://github.com/saqqdy/eslint-sets/tree/master/packages/nuxt#readme"
}
