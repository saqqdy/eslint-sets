{
    "name": "@eslint-sets/eslint-config-nuxt",
    "description": "Eslint config sets for nuxt",
    "version": "2.0.0-beta.1",
    "main": "./index.js",
    "scripts": {},
    "directories": {
        "es": "es",
        "lib": "lib",
        "src": "src",
        "typings": "typings"
    },
    "dependencies": {
        "@eslint-sets/eslint-config-basic": "workspace:*",
        "@nuxtjs/eslint-config": "^9.0.0",
        "@nuxtjs/eslint-module": "^3.0.2",
        "babel-eslint": "^10.1.0",
        "eslint-config-prettier": "^8.5.0",
        "eslint-plugin-jsdoc": "^38.1.4",
        "eslint-plugin-nuxt": "^3.2.0",
        "eslint-plugin-prettier": "^4.0.0",
        "vue-eslint-parser": "^8.3.0"
    },
    "devDependencies": {},
    "peerDependencies": {
        "eslint": ">=8.0.0",
        "prettier": ">=2.4.0"
    },
    "sideEffects": false,
    "repository": {
        "type": "git",
        "url": "git+https://github.com/saqqdy/eslint-sets.git",
        "directory": "packages/nuxt"
    },
    "pnpm": {
        "peerDependencyRules": {
            "ignoreMissing": [
                "@babel/core",
                "webpack"
            ],
            "allowedVersions": {
                "eslint": "^8.10.0"
            }
        }
    },
    "publishConfig": {
        "registry": "https://registry.npmjs.org",
        "access": "public"
    },
    "keywords": [
        "eslint-sets",
        "git",
        "tool"
    ],
    "author": "saqqdy <https://github.com/saqqdy>",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/saqqdy/eslint-sets/issues"
    },
    "homepage": "https://github.com/saqqdy/eslint-sets/tree/master/packages/nuxt#readme"
}
