{
    "name": "@eslint-sets/eslint-config-react-ts",
    "description": "Eslint config sets for react typescript",
    "version": "3.1.0",
    "main": "./index.js",
    "scripts": {},
    "directories": {
        "es": "es",
        "lib": "lib",
        "src": "src",
        "typings": "typings"
    },
    "dependencies": {
        "@eslint-sets/eslint-config-ts": "workspace:*",
        "@typescript-eslint/eslint-plugin": "^5.36.2",
        "@typescript-eslint/parser": "^5.36.2",
        "eslint-config-prettier": "^8.5.0",
        "eslint-plugin-prettier": "^4.2.1",
        "eslint-plugin-react": "^7.31.7",
        "eslint-plugin-react-hooks": "^4.6.0",
        "eslint-plugin-tsdoc": "^0.2.16"
    },
    "devDependencies": {
        "eslint": "^8.30.0",
        "prettier": "^2.8.1",
        "typescript": "~4.8.4"
    },
    "peerDependencies": {
        "eslint": ">=8.0.0",
        "prettier": ">=2.0.0",
        "typescript": ">=4.0.0"
    },
    "sideEffects": false,
    "repository": {
        "type": "git",
        "url": "git+https://github.com/saqqdy/eslint-sets.git",
        "directory": "packages/react-ts"
    },
    "pnpm": {
        "peerDependencyRules": {
            "ignoreMissing": [
                "@babel/core",
                "webpack"
            ],
            "allowedVersions": {
                "eslint": ">=8.0.0"
            }
        }
    },
    "publishConfig": {
        "registry": "https://registry.npmjs.org",
        "access": "public"
    },
    "keywords": [
        "eslint-sets",
        "git",
        "tool"
    ],
    "author": "saqqdy <https://github.com/saqqdy>",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/saqqdy/eslint-sets/issues"
    },
    "homepage": "https://github.com/saqqdy/eslint-sets/tree/master/packages/react-ts#readme"
}
