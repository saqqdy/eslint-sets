{
    "name": "@eslint-sets/simple-ts",
    "description": "js utils for nodejs",
    "version": "1.0.0",
    "exports": {
        ".": {
            "import": "./index.mjs",
            "require": "./index.cjs",
            "types": "./index.d.ts"
        },
        "./*": "./*"
    },
    "main": "./index.cjs",
    "module": "./index.mjs",
    "types": "./index.d.ts",
    "scripts": {},
    "directories": {
        "es": "es",
        "lib": "lib",
        "src": "src",
        "typings": "typings"
    },
    "dependencies": {
        "@eslint-sets/core": "workspace:*",
        "@typescript-eslint/eslint-plugin": "^5.12.1",
        "@typescript-eslint/parser": "^5.12.1",
        "babel-eslint": "^10.1.0",
        "eslint": "^8.12.0",
        "eslint-plugin-tsdoc": "^0.2.14",
        "prettier": "^2.6.1",
        "typescript": "^4.5.5"
    },
    "devDependencies": {},
    "peerDependencies": {
        "eslint": ">=8.0.0",
        "prettier": ">=2.4.0"
    },
    "sideEffects": false,
    "repository": {
        "type": "git",
        "url": "git+https://github.com/saqqdy/eslint-sets.git",
        "directory": "packages/simple-ts"
    },
    "pnpm": {
        "peerDependencyRules": {
            "ignoreMissing": [
                "@babel/core",
                "webpack"
            ],
            "allowedVersions": {
                "eslint": "^8.10.0"
            }
        }
    },
    "publishConfig": {
        "registry": "https://registry.npmjs.org",
        "access": "public"
    },
    "keywords": [
        "eslint-sets",
        "git",
        "tool"
    ],
    "author": "saqqdy <https://github.com/saqqdy>",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/saqqdy/eslint-sets/issues"
    },
    "homepage": "https://github.com/saqqdy/eslint-sets/tree/main/packages/simple-ts#readme"
}