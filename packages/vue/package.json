{
	"name": "@eslint-sets/eslint-config-vue",
	"description": "Eslint config sets for vue2.0",
	"version": "4.2.1",
	"main": "./index.js",
	"scripts": {},
	"files": [
		"index.js",
		"typings",
		"*.js",
		"*.d.ts"
	],
	"directories": {
		"es": "es",
		"lib": "lib",
		"src": "src",
		"typings": "typings"
	},
	"dependencies": {
		"@eslint-sets/eslint-config-basic": "workspace:*",
		"@eslint-sets/eslint-config-ts": "workspace:*",
		"@typescript-eslint/eslint-plugin": "^5.49.0",
		"@typescript-eslint/parser": "^5.49.0",
		"eslint-config-prettier": "^8.6.0",
		"eslint-plugin-jsdoc": "^39.7.4",
		"eslint-plugin-prettier": "^4.2.1",
		"eslint-plugin-tsdoc": "^0.2.17",
		"eslint-plugin-vue": "^9.9.0",
		"eslint-plugin-vue-scoped-css": "^2.4.0",
		"local-pkg": "^0.4.3",
		"vue-eslint-parser": "^9.1.0"
	},
	"devDependencies": {
		"eslint": "^8.33.0",
		"prettier": "^2.8.3",
		"typescript": "~4.9.5"
	},
	"peerDependencies": {
		"eslint": ">=7.4.0",
		"prettier": ">=2.0.0"
	},
	"keywords": [
		"eslint-sets",
		"git",
		"tool"
	],
	"license": "MIT",
	"author": "saqqdy <https://github.com/saqqdy>",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/saqqdy/eslint-sets.git",
		"directory": "packages/vue"
	},
	"pnpm": {
		"peerDependencyRules": {
			"ignoreMissing": [
				"@babel/core",
				"webpack"
			],
			"allowedVersions": {
				"eslint": ">=7.4.0"
			}
		}
	},
	"publishConfig": {
		"registry": "https://registry.npmjs.org",
		"access": "public"
	},
	"bugs": {
		"url": "https://github.com/saqqdy/eslint-sets/issues"
	},
	"homepage": "https://github.com/saqqdy/eslint-sets/tree/master/packages/vue#readme"
}
