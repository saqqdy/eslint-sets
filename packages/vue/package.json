{
    "name": "@eslint-sets/vue",
    "description": "js utils for nodejs",
    "version": "1.0.0-alpha.2",
    "exports": {
        ".": {
            "import": "./index.mjs",
            "require": "./index.cjs",
            "types": "./index.d.ts"
        },
        "./*": "./*"
    },
    "main": "./index.cjs",
    "module": "./index.mjs",
    "types": "./index.d.ts",
    "scripts": {},
    "directories": {
        "es": "es",
        "lib": "lib",
        "src": "src",
        "typings": "typings"
    },
    "dependencies": {
        "@eslint-sets/core": "workspace:*",
        "babel-eslint": "^10.1.0",
        "eslint": "^8.12.0",
        "eslint-config-prettier": "^8.5.0",
        "eslint-plugin-import": "^2.25.4",
        "eslint-plugin-jsdoc": "^38.1.4",
        "eslint-plugin-prettier": "^4.0.0",
        "eslint-plugin-vue": "^8.5.0",
        "eslint-plugin-vue-scoped-css": "^2.2.0",
        "prettier": "^2.6.1",
        "vue-eslint-parser": "^8.3.0"
    },
    "devDependencies": {},
    "peerDependencies": {
        "eslint": ">=8.0.0",
        "prettier": ">=2.4.0"
    },
    "sideEffects": false,
    "repository": {
        "type": "git",
        "url": "git+https://github.com/saqqdy/eslint-sets.git",
        "directory": "packages/vue"
    },
    "pnpm": {
        "peerDependencyRules": {
            "ignoreMissing": [
                "@babel/core",
                "webpack"
            ],
            "allowedVersions": {
                "eslint": "^8.10.0"
            }
        }
    },
    "publishConfig": {
        "registry": "https://registry.npmjs.org",
        "access": "public"
    },
    "keywords": [
        "eslint-sets",
        "git",
        "tool"
    ],
    "author": "saqqdy <https://github.com/saqqdy>",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/saqqdy/eslint-sets/issues"
    },
    "homepage": "https://github.com/saqqdy/eslint-sets/tree/master/packages/vue#readme"
}