{
	"name": "@eslint-sets/eslint-config-vue",
	"description": "Eslint config sets for vue2.0",
	"version": "5.3.0",
	"main": "./dist/index.js",
	"typings": "./dist/index.d.ts",
	"files": [
		"bin",
		"dist"
	],
	"scripts": {},
	"directories": {
		"es": "es",
		"lib": "lib",
		"src": "src",
		"typings": "typings"
	},
	"dependencies": {
		"@eslint-sets/eslint-config-basic": "workspace:*",
		"@eslint-sets/eslint-config-ts": "workspace:*",
		"@typescript-eslint/eslint-plugin": "^5.59.7",
		"@typescript-eslint/parser": "^5.59.7",
		"eslint-config-prettier": "^8.8.0",
		"eslint-plugin-jsdoc": "^46.2.4",
		"eslint-plugin-prettier": "^4.2.1",
		"eslint-plugin-tsdoc": "^0.2.17",
		"eslint-plugin-vitest-globals": "^1.3.1",
		"eslint-plugin-vue": "^9.14.1",
		"eslint-plugin-vue-scoped-css": "^2.4.0",
		"local-pkg": "^0.4.3",
		"vue-eslint-parser": "^9.3.0"
	},
	"devDependencies": {
		"eslint": "^8.41.0",
		"prettier": "^2.8.8",
		"typescript": "~5.0.4"
	},
	"peerDependencies": {
		"eslint": ">=7.4.0",
		"prettier": ">=2.0.0"
	},
	"peerDependenciesMeta": {
		"typescript": {
			"optional": true
		}
	},
	"pnpm": {
		"peerDependencyRules": {
			"ignoreMissing": [
				"@babel/core",
				"webpack"
			],
			"allowedVersions": {
				"eslint": ">=7.4.0"
			}
		}
	},
	"keywords": [
		"eslint-sets",
		"git",
		"tool"
	],
	"license": "MIT",
	"author": "saqqdy <https://github.com/saqqdy>",
	"homepage": "https://github.com/saqqdy/eslint-sets/tree/master/packages/vue#readme",
	"bugs": {
		"url": "https://github.com/saqqdy/eslint-sets/issues"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/saqqdy/eslint-sets.git",
		"directory": "packages/vue"
	},
	"publishConfig": {
		"registry": "https://registry.npmjs.org",
		"access": "public"
	}
